{
  "name": "arquero",
  "version": "0.0.1",
  "description": "Query processing and transformation of array-backed data tables.",
  "keywords": [
    "query",
    "array",
    "column",
    "database",
    "data",
    "frame",
    "table",
    "transformation"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "main": "build/arquero.js",
  "module": "build/arquero.mjs",
  "scripts": {
    "rollup": "rollup -c",
    "prebuild": "rimraf build && mkdir build",
    "build": "yarn rollup",
    "postbuild": "terser build/arquero.js -c -m -o build/arquero.min.js",
    "lint": "yarn eslint src test --ext .js",
    "test": "tape 'test/**/*-test.js' --require esm",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "acorn": "^8.0.1",
    "d3-dsv": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "eslint": "^7.7.0",
    "esm": "^3.2.25",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.8",
    "rollup-plugin-terser": "^7.0.1",
    "tape": "^5.0.1"
  }
}
